      "optional": true
    },
    "vue-template-compiler": {
      "optional": true
    }
  },
  "dependencies": {
    "@iconify/json-tools": "^1.0.10",
    "vue-template-es2015-compiler": "^1.9.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.6.5",
    "@types/node": "^15.0.3",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@vue/compiler-sfc": "^3.0.11",
    "eslint": "^7.26.0",
    "rollup": "^2.47.0",
    "tsup": "^4.10.1",
    "typescript": "^4.2.4",
    "vite": "^2.1.5",
    "vue": "^3.0.11",
    "vue-template-compiler": "^2.6.12",
    "vue-template-es2015-compiler": "^1.9.1"
  }
}
